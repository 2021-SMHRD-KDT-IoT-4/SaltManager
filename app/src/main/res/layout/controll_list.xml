<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_controll_numbering"
        android:layout_width="40dp"
        android:layout_height="80dp"
        android:background="#EEF6FB"
        android:fontFamily="@font/nanumsquare_aceb"
        android:gravity="center"
        android:text="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/tv_controll_zk"
        android:layout_width="250dp"
        android:layout_height="80dp"
        android:background="#EEF6FB"
        android:fontFamily="@font/nanumsquare_aceb"
        android:gravity="center"
        android:text="결정지"
        app:layout_constraintStart_toEndOf="@+id/tv_controll_numbering"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView160"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:background="#EEF6FB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_controll_move"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:backgroundTint="#7DBDFE"
        android:fontFamily="@font/nanumsquare_aceb"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:text="제어"
        app:layout_constraintBottom_toBottomOf="@+id/textView160"
        app:layout_constraintEnd_toEndOf="@+id/textView160"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_controll_zk"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>